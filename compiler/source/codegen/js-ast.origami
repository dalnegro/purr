% language: origami

import "@babel/types" as t;
import "../utils/array" exposing map;


module Statement {
  data class Block(statements) {
    member _.asBabelAST = t.blockStatement(map(statements, (x) => x.asBabelAST));
  }

  data class Break() {
    member _.asBabelAST = t.breakStatement();
  }

  data class BreakToLabel(label) {
    member _.asBabelAST = t.breakStatement(label);
  }


}