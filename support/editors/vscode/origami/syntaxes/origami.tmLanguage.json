{
  "$schema":
    "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "Origami",
  "patterns": [
    {
      "name": "keyword.control.origami",
      "match":
        "(?<![a-zA-Z0-9_\\.\\^])(import|as|exposing|define|end|where|assert|law|forall|async|await|generator|yield|union|abstract|class|extends|implements|let|mutable|include|without|renaming|member|static|export|match|with|if|then|else|do|foreach|of|repeat|from|to|by|break|continue|try|catch|case|const|debugger|default|delete|enum|finally|for|function|in|instanceof|new|return|super|throw|switch|this|typeof|var|void|while|interface|package|private|protected|public|required|optional|override|protocol|implement|and|or|not)(?![a-zA-Z0-9_])"
    },
    {
      "name": "constant.language.origami",
      "match": "(?<![a-zA-Z0-9_])(true|false)(?![a-zA-Z0-9_])"
    },
    {
      "name": "comment.line.heading.origami",
      "match": "%[^\\n\\r]*"
    },
    {
      "name": "constant.numeric.integer.binary.origami",
      "match": "(?<![a-zA-Z0-9_])[\\-\\+]?0b[01][01_]*"
    },
    {
      "name": "constant.numeric.integer.octal.origami",
      "match": "(?<![a-zA-Z0-9_])[\\-\\+]?0o[0-7][0-7_]*"
    },
    {
      "name": "constant.numeric.integer.hexadecimal.origami",
      "match": "(?<![a-zA-Z0-9_])[\\-\\+]?0x[0-9a-fA-F][0-9a-fA-F_]*"
    },
    {
      "name": "constant.numeric.integer.origami",
      "match": "(?<![a-zA-Z0-9_])[\\-\\+]?[0-9][0-9_]*"
    },
    {
      "name": "constant.numeric.decimal.origami",
      "match":
        "(?<![a-zA-Z0-9_])[\\-\\+]?[0-9][0-9_]*(\\.[0-9][0-9_]*)?([eE][\\-\\+]?[0-9][0-9_]*)?"
    },
    {
      "name": "storage.type.origami",
      "match": "\\^[a-zA-Z][a-zA-Z0-9_\\.]*"
    },
    {
      "name": "variable.name.origami",
      "match": "(?<![a-zA-Z0-9_])[a-zA-Z][a-zA-Z0-9_]*"
    },
    {
      "name": "keyword.symbol.origami",
      "match": "'[a-zA-Z][a-zA-Z0-9_]*"
    },
    {
      "name": "string.quoted.triple",
      "begin": "\"{3}",
      "end": "\"{3}"
    },
    {
      "name": "string.quoted.double",
      "begin": "\"",
      "end": "\""
    },
    {
      "name": "comment.line",
      "match": "#.+"
    },
    {
      "name": "keyword.operator.origami",
      "match":
        "(?<![a-zA-Z0-9\\-])(==>|\\->|=>|===|=/=|>|>=|<|<=|\\+|\\-|\\*|/|@|\\|>|`|\\$)(?!a-zA-Z0-9\\-)"
    }
  ],
  "scopeName": "source.origami"
}
