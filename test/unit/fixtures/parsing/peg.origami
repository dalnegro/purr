% version: 1
% pass: true

function main() {
  @origami.grammar.peg"""
    Arithmetic {
      Expr
        = a:Expr "+" b:Expr { new Add(a, b); }
        / a:Expr "*" b:Expr { new Mul(a, b); }
        / a:Expr "-" b:Expr { new Sub(a, b); }
        / a:Expr "/" b:Expr { new Div(a, b); }
        / Prim
        ;

      Prim
        = Number
        / "(" a:Expr ")" { a; }
        ;

      Number
        = x:digit+ { new Num(Number(x.join(""))); }
        ;
    }
  """;
}