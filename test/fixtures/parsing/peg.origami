% version: 1

function main() {
  <origami.grammar.peg>"""
    Arithmetic {
      Expr
        = Expr "+" Expr { (a, _, b) => new Add(a, b) }
        / Expr "*" Expr { (a, _, b) => new Mul(a, b) }
        / Expr "-" Expr { (a, _, b) => new Sub(a, b) }
        / Expr "/" Expr { (a, _, b) => new Div(a, b) }
        / Prim
        ;

      Prim
        = Number
        / "(" Expr ")" { (_, a, _) => a }
        ;

      Number
        = digit+ { (x) => x.join("") }
        ;
    }
  """;
}